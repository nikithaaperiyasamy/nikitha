def repeat_characters(s):
    result = ''
    i = 0
    while i < len(s):
        if s[i].isalpha():
            char = s[i]
            i += 1
            repeat_count = 0
            while i < len(s) and s[i].isdigit():
                repeat_count = repeat_count * 10 + int(s[i])
                i += 1
            result += char * repeat_count
        else:
            i += 1
    return result
user_input = input()
print(repeat_characters(user_input))
